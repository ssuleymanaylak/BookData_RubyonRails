<div class="w-4/5 mx-auto">
  <h1 class="mx-auto text-2xl w-fit">The Library</h1>
  <%= turbo_frame_tag "books" do %>
  <table>
    <tbody>
      <% @books.each do |book| %>
      <tr>
        <td><%= book.title %></td>
        <td><%= book.author %></td>
        <td><%= book.publisher %></td>
        <td><%= book.category %></td>
      </tr>
      <% end %>
    </tbody>
  </table>

  <% end %>

</div>
